.container{
  background-color: rgb(245, 245, 245);
  padding: 0 10px;
}
.card {
    width: 100%;
    margin-top: 8px;
    margin-bottom: 8px;
    background-color: #FFF; 
}
.card-image{
    width: 100%;
    height: 200px;
}

.card-content{
    padding-left: 16px;
    padding-right: 16px;
    padding-bottom: 10px;
}

.card-title{
    font-size: 16px;
    line-height: 1.6;
}

.card-desc {
    font-size: 14px;
}
.fnav_l{
  display: flex;
}
.fnav_h{
  flex: 1;
  font-size: 12px;
  color: #999;
  text-align: center;
  height: 50px;
  line-height: 50px;
  border-right: 1px solid rgb(245, 245, 245);
  box-shadow:0 4px 2px rgb(245, 245, 245);
}